#URL para la BDD, junto al nombre que tiene la misma
spring.datasource.url=jdbc:mysql//localhost:3306/db_microservicios_examenes
#Usuario
spring.datasource.username=root
#Contrasenia
spring.datasource.password=Scenic9513HBM
#Driver utilizado
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
#Dialecto para MySQL
spring.jpa.database-platform=org.hibernate.dialect.MySQL8Dialect
#Crear las tablas automáticamente en base a las anotaciones o metadatas dadas
spring.jpa.generate-ddl=true

 
